<Application x:Class="CottonDBMS.BridgeApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CottonDBMS.BridgeApp"
             xmlns:converters="clr-namespace:CottonDBMS.Bridges.Shared.Converters;assembly=CottonDBMS.Bridges.Shared"    
             StartupUri="MainWindow.xaml" Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/CottonDBMS.Bridges.Shared;component/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
